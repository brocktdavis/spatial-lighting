{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "func-style": ["warn", "declaration"],
        "array-element-newline":"off",
        "implicit-arrow-linebreak":"off",
        "no-magic-numbers":"off",
        "indent": ["error", 2],
        "react/no-unknown-property":"off",
        "react/prop-types":"off"
    }
}
